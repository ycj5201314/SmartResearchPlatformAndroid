            android:gravity="left">
        </clip>
    </item>

    <item android:duration="300">
        <clip
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:clipOrientation="horizontal"
            android:drawable="@drawable/loading_0"
            android:gravity="left">
        </clip>
    </item>
</animation-list>